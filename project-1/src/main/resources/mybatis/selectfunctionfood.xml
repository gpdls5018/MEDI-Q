<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.kosmo.springapp.service.SelectFoodMapper">
	<select id="selectFoodListFormName" parameterType="String" resultType="FunctionalFoodListDTO">
	    select * from foodtable where (productname LIKE '%${foodname}%') OR (company LIKE '%${foodname}%') ORDER by imgurl
	</select>
</mapper>